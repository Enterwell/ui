{
  "name": "@enterwell/react-ui",
  "description": "Enterwell's React UI component library.",
  "version": "0.14.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./api": "./dist/api.js",
    "./package": "./dist/package.js"
  },
  "license": "MIT",
  "scripts": {
    "dev:js": "tsup index.tsx --format esm --dts --watch",
    "dev": "pnpm run \"/^dev:.*/\"",
    "before-build:clean": "rimraf dist",
    "build:js": "tsup index.tsx --format esm --dts",
    "after-build:api": "api-extractor run && node ./scripts/transform-api-docs.js",
    "build": "pnpm run \"/^before-build:.*/\" && pnpm run \"/^build:.*/\" && pnpm run \"/^after-build:.*/\"",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.0",
    "@enterwell/react-hooks": "workspace:*",
    "@microsoft/api-extractor": "7.52.1",
    "@mui/icons-material": "7.0.1",
    "@mui/material": "7.0.1",
    "@mui/system": "7.0.1",
    "@mui/x-date-pickers": "7.27.3",
    "@mui/x-date-pickers-pro": "7.27.3",
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "date-fns": "4.1.0",
    "eslint-config-custom": "workspace:*",
    "react": "19.0.0",
    "rimraf": "6.0.1",
    "tsconfig": "workspace:*",
    "tsup": "8.4.0",
    "typescript": "5.8.2"
  },
  "peerDependencies": {
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@mui/material": "^6 || ^7.0.0",
    "@mui/system": "^6 || ^7.0.0",
    "@mui/x-date-pickers": "^6 || ^7.0.0",
    "@mui/x-date-pickers-pro": "^6 || ^7.0.0",
    "date-fns": "^2",
    "react": "^18",
    "react-dom": "^18"
  }
}